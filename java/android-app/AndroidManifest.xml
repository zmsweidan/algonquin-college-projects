<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
      <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".patient_intake.PatientIntakeStart" />
        <activity android:name=".patient_intake.PatientIntakeForm" />
        <activity android:name=".patient_intake.PatientIntakeImport" />
        <activity android:name=".patient_intake.PatientIntakeManage" />

        <!-- Multiple Choice Creator Activities - Prefix: Mcc -->
        <activity android:name=".quiz.MccMainActivity" />
        <!-- End Multiple Choice Creator Strings -->

        <activity android:name=".quiz.MccQuestionActivity"/>
        <activity
            android:name=".octranspo.AC_OCTranspActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity android:name=".patient_intake.PatientIntakeStatistics"></activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>